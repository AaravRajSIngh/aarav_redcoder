//Solution 1 accepted

class Solution {
public:
    void reverseString(vector<char>& s) {
        
        
        reverse(s.begin(),s.end());
    }
};



//solution 2 accepted

class Solution {
public:
    void reverseString(vector<char>& s) {
        
        
        
        int first=0,last=s.size()-1;
        
        
        while(first<last)
        {
            swap(s[first++],s[last--]);
        }
        
    }
};
